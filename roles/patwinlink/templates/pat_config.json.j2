{
  "mycall": "{{ callsign }}",
  "secure_login_password": "{{ winlink_password }}",
  "auxiliary_addresses": [],
  "locator": "{{ gridsquare }}",
  "service_codes": [
    "PUBLIC"
  ],
  "http_addr": "0.0.0.0:8080",
  "motd": [
    "Open source Winlink client - getpat.io"
  ],
  "connect_aliases": {
    "telnet": "telnet://{mycall}:CMSTelnet@cms.winlink.org:8772/wl2k"
  },
  "listen": ["ax25"],
  "hamlib_rigs": {
    "hamlib_radio": {"address": "localhost:4532", "network": "tcp"}
  },
  "ax25": {
    "port": "wl2k",
    "beacon": {
      "every": 3600,
      "message": "Winlink P2P",
      "destination": "IDENT"
    },
    "rig": "hamlib_radio"
  },
  "serial-tnc": {
    "path": "/dev/ttyACM0",
    "baudrate": 9600,
    "type": "IC-7300"
  },
  "ardop": {
    "addr": "localhost:8515",
    "arq_bandwidth": {
      "Forced": false,
      "Max": 500
    },
    "rig": "hamlib_radio",
    "ptt_ctrl": true,
    "beacon_interval": 0,
    "cwid_enabled": true
  },
  "telnet": {
    "listen_addr": ":8774",
    "password": ""
  },
  "gpsd": {
    "enable_http": true,
    "use_server_time": false,
    "addr": "localhost:2947"
  },
  "schedule": {},
  "version_reporting_disabled": false
}
